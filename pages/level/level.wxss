/**level.wxss**/  
page{
  display: block;
  min-height: 100%;
  overflow: hidden;
  position: fixed;
  background-size: 100% 100%;
  margin: 0px;
  background-image: url('http://static3-cdn.scloud.lfengmobile.com/800000026-mokao/challenge-bg.png');
}

.lock-grid {  
  position: relative;  
  float: left;
  width: 20%;
  background:#f0f0f0; 
  margin: 10rpx 10rpx 2rpx 25rpx;
  border-radius: 20rpx;
} 
.lock-grid:active {  
  background-color: #eec790;
}
.gap{
  width: 100%;
  min-height: 100%;
}
.unlock-grid {
  padding-top: 50rpx;
  border-radius: 10rpx;
}

.class {
  display: block;  
  text-align: center;
  color: #eec790;
  font-size: 1.2rem;
  margin-left: 10%;
  margin-right: 10%;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  background:#f0f0f0;
  border-radius: 80rpx;
}
.lock:active {  
  background-color: #eec790;
}

.control{
    display: block;
    width: 100%;
    height: 100%;
}