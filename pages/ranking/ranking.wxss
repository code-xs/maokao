/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userimg {
  width: 108rpx;
  height: 108rpx;
  margin-right: 20rpx; 
  display: flex;
  border-radius: 40%;
}

.userimg_crown {
  width: 48rpx;
  height: 48rpx;
  margin-left: -50rpx;
  margin-top: -96rpx;
}

.userinfo-nickname {
  color: #FFFFFF;
}

.usermotto {
  margin-top: 200px;
}

page{
  display: block;
  min-height: 100%;
  overflow: hidden;
  position: fixed;
  background-size: 100% 100%;
  margin: 0px;
  background-image: url('https://lg-6enwjric-1256925828.cos.ap-shanghai.myqcloud.com/rank/bg_rank.png');
}

.canvas{
    width: 100%;
    margin-left: 20rpx;
}
.invite{
  padding-top: 20rpx; 
}

.canvasOverflow{
    width: 100%;
}
.control{
    width: 100%;
    height: 100%;
}
.score {
    display: flex;
    margin-left: 1rpx;
    margin-right: 3rpx;
    /*background:#285872;*/
}
.name{
  color: #FFFFFF;
  text-align: left;
  flex-direction: column;  
  justify-content: center; 
  font-size: 0.8rem;
}
.level{
  color: rgba(246, 249, 252,0.5);
  text-align: left;
  flex-direction: column;  
  justify-content: center; 
  margin-top: 10rpx;
  font-size: 0.8rem;
}
.score1 {
    display: flex;
    margin-left: 5%;
    margin-right: 5%;
}
.rule{
    flex:1;
    background:#173645;
    color:#ff3429;
    margin: 2rpx 2rpx;
    text-align: center;
    border-radius: 2rpx;  
}
.list{
  margin-top: 2rpx;
}

.list_bg{
  margin-top: 30rpx;
  margin-left: 5%;
  margin-right: 5%;
  border-radius: 50rpx;
  height: 1020rpx;
}
.group1{
    flex:1;
}
.group0{
  flex:4;
  display: flex;  
  flex-direction: column;  
  justify-content: center;     
}
.sub_group{  
  flex-direction: row;
}  

.rate-title{
  flex:2;
  display: flex;  
  flex-direction: column;  
  justify-content: center; 
  text-align: right;
  color: rgba(246, 249, 252,0.5);
  opacity: 1;
}

.rate{
  flex:2;
  color:#8755de;
  font-size: 1.1em;
  margin-right: 10rpx;
  display: flex;  
  flex-direction: column;  
  justify-content: center; 
  text-align: center;
  opacity: 1;
}
.title{
    flex:1;
    height: 120rpx;
    line-height: 120rpx;
    background:#285872;
    color:aliceblue;
    margin: 10rpx 10rpx 10rpx 10rpx;
    text-align: center;
    border-radius: 8rpx;
}
.data{
  color: #e0a30a;
}
scroll-view {
  height: 720rpx;
}
 
.item-content {
    width: 80%;
    display: inline-block;
    border: 1rpx solid gray;
    text-align: center;
}
.item{
    display: flex;
    /*background: #4b4b4b;*/
    justify-content:center;
    align-items: center;
    font-size: 0.9rem;
    color: #f5ede4;
    height: 160rpx;
}
.showRanking{
  color:aliceblue;
  background: #8B44C9;
  margin: 20rpx 20% 20rpx 20%;
  padding:20rpx;
  max-height: 100rpx;
  text-align: center;
  border-radius: 40rpx;
  font-size: 0.9rem;
}
.scoredetail1{
    flex:1;
    color:aliceblue;
    margin: 10rpx;
    padding: 5rpx;
    text-align: center;
}

.buttonsStyle{
    flex:1;
    background:#edab63;
    color:aliceblue;
    margin: 2rpx 20rpx;
    text-align: center;
    border-radius: 10rpx;
    font-size: 0.8rem;
    font-weight: bold;
}
.buttonsStyle:active {  
  background-color: #e0a30a;
}
.nametitle{
    font-weight: bold;
    font-size: 1.0rem;
    color: #000000;
    display: flex;
    justify-content: center;
}

.rankingitem{
    font-size: 0.9rem;
    color: #7f8fa6;
    flex:1;
    text-align: center;
    margin: 20rpx;
    padding: 20rpx;
    background: #ffffff;
    border-radius: 40rpx;
}

.rankingitem-active{
    font-size: 0.9rem;
    color: #ffffff;
    flex:1;
    text-align: center;
    margin: 20rpx;
    padding: 20rpx;
    background: #8755de;
    border-radius: 40rpx;
}
.ranking{
    width: 44rpx;
    height: 44rpx;  
    font-size: 1.0rem;
    text-align: center;
    margin-left: 20rpx;
    margin-right: 20rpx;
    color: #ffffff;
    display: flex;  
    flex-direction: column;  
    justify-content: center;  
    align-items: center;
    border-radius: 50%;
}

.empirical{
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    padding-left: 20rpx;
    font-size: 0.8rem;
    line-height: 40rpx;
    color: #000000;
}
.subtitle{
    padding-top: 10rpx;
    padding-left: 20rpx;
    padding-right: 20rpx;
    font-weight: bold;
    font-size: 0.8rem;
    line-height: 40rpx;
    color: #f5ede4;
}
.scorenumber{
    font-size: 1.0rem;
    line-height: 50rpx;
    text-align: center;
}

.nodeTitle{
    font-size: 1.0rem;
    line-height: 40rpx;
    font-weight: bold;
}
.node{
    padding-top: 10rpx;
    font-size: 1.2rem;
    line-height: 50rpx;
    text-align: center;
}

.descmax{
    font-size: 0.8rem;
    line-height: 40rpx;
}
.scoremax{
    line-height: 50rpx;
    text-align: center;
}

.modal-dialog {
  width: 700rpx;
  overflow: hidden;
  position: fixed;
  top: 60%;
  z-index: 9999;
  background: #252525;
  margin: 20rpx 20rpx;
  border-radius: 36rpx;
}

.modal-title {
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  font-size: 36rpx;
  color: #dedede;
  text-align: center;
}

.modal-content {
  padding: 50rpx 32rpx;
}

.modal-footer {
  display: flex;
  flex-direction: row;
  height: 86rpx;
  border-top: 1px solid #dedede;
  font-size: 34rpx;
  line-height: 86rpx;
}

.btn-normal {
  width: 50%;
  color: #dedede;
  text-align: center;
}

.btn-select {
  width: 50%;
  color: aliceblue;
  text-align: center;
  border-right: 1px solid #dedede;
  border-left: 1px solid #dedede;
}

.retryError{
  width: 100%;
  margin-top: 50rpx;
  text-align: center;
  font-size: 0.8rem; 
  text-decoration:underline; 
}
.item0 {
    display: flex;
    color: #151515;
    background: #f5ede4;
    margin: 20rpx 50rpx 20rpx 50rpx;
    padding:20rpx;
    border-radius: 50rpx;    
}
.group{
    position: relative;  
    float: left;
    color:aliceblue;
    margin: 15rpx 40rpx 15rpx 40rpx;
    text-align: center;
}


.showScore{
    width: 56%;
    height: 80rpx;
    display: flex;
    justify-content:center;
    align-items: center;
    background-size: 100% 100%;
    margin-top: 30rpx;
    color: #FFFFFF;
    font-size: 1.1rem;
    font-weight: bold;
    background-repeat: no-repeat;
    background-image: url(' https://lg-6enwjric-1256925828.cos.ap-shanghai.myqcloud.com/my/btn_me_show.png');
    background-color: transparent;
}

.myRanking{
  padding-top: 10rpx;
  color: rgba(246, 249, 252,0.5);
  margin-left: 100rpx;
}

.cateoryRanking{
  padding-top: 5rpx;
  padding-bottom: 5rpx;
  color: #ffffff;
  margin-left: 100rpx;
  justify-content: center;    
}
.myRankingItem{
  display: flex;
  padding-top: 10rpx;
  color: rgba(246, 249, 252,0.5);
  font-size: 0.8rem;
}
.cateoryItem{
  display: flex;
  padding-top: 20rpx;
  color: #ffffff;
  font-size: 0.9rem;
}
.cateoryItemHide{
  display: none;
}
.arrow{
  width: 50rpx;
  height: 100%;
  margin-left: 20rpx;
  flex: 1;
  display: flex;  
  flex-direction: column;  
  justify-content: center;  
  align-items: center;
}
.selectItem{
  display: flex;
  background: rgba(246, 249, 252,0.2);
  border-radius: 30rpx;
}
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}
.cateoryList{
  overflow: hidden;
  position: fixed;
  margin-left: 10%;
  width: 80%;
  top: 20%;
  z-index: 9001;
  background: #bb6ed5;
  border-radius: 30rpx;
}
.cateoryText{
  font-size: 1.0rem;
  text-align: center;
  background: #ffffff;
  padding-top: 15rpx;
  padding-bottom: 15rpx;
  width: 90%;
  margin-top: 20rpx;
  margin-left: 5%;
  margin-right: 5%;
  border-radius: 40rpx;
  color: #bb6ed5;
}

::-webkit-scrollbar{
width: 0;
height: 0;
color: transparent;
}